pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World out'
            }
        }
    }
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World in'
            }
        }
    }
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World here'
            }
        }
    }

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World over'
            }
        }
    }



}